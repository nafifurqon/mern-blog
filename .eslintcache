\n        setCounter(counter <= 1 ? 1 : counter - 1 );\n        console.log('counter', counter);\n    }\n\n    const next = () => {\n        setCounter(counter === page.totalPage ? page.totalPage : counter + 1);\n        console.log('counter', counter);\n    }\n\n    return (\n        <div className=\"home-page-wrapper\">\n            <div className=\"create-wrapper\">\n                <Button title=\"create blog\" onClick={() => history.push('/create-blog')} />\n            </div>\n            <Gap height={20} />\n            <div className=\"content-wrapper\">\n                {dataBlog.map(blog => {\n                    return <BlogItem \n                    key={blog._id} \n                    image={`http://localhost:4000/${blog.image}`} \n                    title={blog.title} \n                    body={blog.body}\n                    name={blog.author.name} \n                    date={blog.createdAt} />\n                })}\n            </div>\n            <div className=\"pagination\">\n                <Button title=\"previous\" onClick={previous} />\n                <Gap width={20} />\n                <p className=\"text-page\">{page.currentPage} / {page.totalPage}</p>\n                <Gap width={20} />\n                <Button title=\"next\" onClick={next} />\n            </div>\n            <Gap height={20} />\n        </div>\n    )\n}\n\nexport default Home\n",["126","127"],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/config/redux/store.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/config/redux/reducer/globalReducer.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/pages/Login/index.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/assets/index.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/components/atoms/TextArea/index.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/components/atoms/Button/index.js",[],"/home/nafifurqon/2019/belajar/prawito/mern-blog/src/config/redux/reducer/reducer.